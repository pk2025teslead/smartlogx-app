{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/hello",
      "dest": "api/hello.py"
    },
    {
      "src": "/test",
      "dest": "api/test.py"
    },
    {
      "src": "/debug",
      "dest": "api/debug.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}